{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "cd client && CI=false npm install && CI=false npm run build",
  "outputDirectory": "client/build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://locally.onrender.com/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
