{
  "version": 2,
  "name": "locally-frontend",
  "builds": [
    {
      "src": "client/build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://locally.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    },
    {
      "src": "/",
      "dest": "/client/build/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ]
}
