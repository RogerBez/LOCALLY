{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://locally-server.onrender.com/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CI": "false"
  }
}
